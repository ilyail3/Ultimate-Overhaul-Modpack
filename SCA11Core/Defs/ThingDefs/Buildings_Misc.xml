			<fixedStorageSettings>
			<priority>Important</priority>
			<filter>
				<categories>
					<li>FoodRaw</li>
				</categories>
			</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
			<priority>Important</priority>
			<filter>
				<categories>
					<li>FoodRaw</li>
				</categories>
